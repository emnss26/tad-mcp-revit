{"action_key": "graphics.show_category_in_view", "templates": [{"prompt": "Muestra la categoría '{category}' en la vista '{view}'", "plan": {"action": "graphics.set_category_visibility", "args": {"category": "{category_en}", "visible": true, "view": "{view}"}}}]}
