{"action_key": "views.create_plan_if_not_exists", "templates": [{"prompt": "Asegúrate de que exista una vista de planta de '{discipline}' para el nivel '{level_name}'", "plan": [{"action": "get.view_by_name", "args": {"name": "{level_name} - {discipline}"}}, {"action": "views.create_plan", "condition": {"ref": 0, "operator": "is_null"}, "args": {"level_name": "{level_name}", "discipline": "{discipline_en}"}}]}, {"prompt": "Crea una vista de planta para '{level_name}' si no existe ya una", "plan": [{"action": "get.view_by_name", "args": {"name": "{level_name}"}}, {"action": "views.create_plan", "condition": {"ref": 0, "operator": "is_null"}, "args": {"level_name": "{level_name}"}}]}]}
