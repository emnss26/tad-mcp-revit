{"action_key": "get.builtin_parameter_selection", "templates": [{"prompt": "Quiero saber el valor del parámetro 'builtin' '{builtin_name}' para el elemento que voy a seleccionar", "plan": [{"action": "ui.prompt_for_selection", "args": {"prompt": "Selecciona un elemento para consultar su parámetro interno."}}, {"action": "get.parameter_value", "args": {"source": {"ref": 0}, "builtin": "{builtin_name}"}}]}, {"prompt": "Dime el valor del parámetro del sistema '{builtin_name}' del siguiente objeto que elija", "plan": [{"action": "ui.prompt_for_selection", "args": {"prompt": "Selecciona un elemento."}}, {"action": "get.parameter_value", "args": {"source": {"ref": 0}, "builtin": "{builtin_name}"}}]}]}
