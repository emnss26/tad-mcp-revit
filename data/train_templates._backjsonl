{"action_key": "export.elements_by_category_to_csv", "templates": [{"prompt": "Exporta a CSV todas las '{category}' con los campos {fields_list} a la ruta '{path}'", "plan": {"action": "export.elements_to_csv", "args": {"path": "{path}", "fields": "{fields}", "source": {"category": "{category_en}"}}}}]}
{"action_key": "elements.rename_grid_selection", "templates": [{"prompt": "Quiero renombrar el eje que voy a seleccionar a '{new_name}'", "plan": [{"action": "ui.prompt_for_selection", "args": {"prompt": "Selecciona un Eje (Grid)."}}, {"action": "elements.set_parameter", "args": {"source": {"ref": 0}, "name": "Name", "value": "{new_name}"}}]}, {"prompt": "Permíteme seleccionar un eje y cambiarle el nombre a '{new_name}'", "plan": [{"action": "ui.prompt_for_selection", "args": {"prompt": "Selecciona un eje."}}, {"action": "elements.set_parameter", "args": {"source": {"ref": 0}, "name": "Name", "value": "{new_name}"}}]}]}
